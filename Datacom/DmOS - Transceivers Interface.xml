                            <type>SNMP_AGENT</type>
                            <snmp_oid>DMOS-TRANSCEIVER-MIB::laneRxPower.{#SNMPINDEX}.1</snmp_oid>
                            <key>laneRxPower[{#SNMPINDEX}]</key>
                            <delay>10m</delay>
                            <value_type>FLOAT</value_type>
                            <units>dBm</units>
                            <applications>
                                <application>
                                    <name>Transceivers</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>0.01</params>
                                </step>
                            </preprocessing>
