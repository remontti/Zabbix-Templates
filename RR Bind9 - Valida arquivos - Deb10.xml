            <items>
                <item>
                    <name>named.conf.local  $1</name>
                    <key>vfs.file.cksum[/etc/bind/named.conf.local]</key>
                    <delay>60s</delay>
                    <history>1d</history>
                    <trends>7d</trends>
                    <applications>
                        <application>
                            <name>Segurança</name>
                        </application>
                    </applications>
                    <request_method>POST</request_method>
